#!/bin/sh

if [ "$KDE_FULL_SESSION" = "true" ] && [ -x /usr/bin/apturl-kde ]; then
    apturl-kde $@
elif [ -x /usr/bin/apturl-elm ]; then
    esudo apturl-elm $@
elif [ -x /usr/bin/apturl-gtk ]; then
    apturl-gtk $@
elif [ -x /usr/bin/apturl-kde ]; then
    apturl-kde $@
else
    echo "Please install apturl-gtk, apturl-kde or apturl-elm."
fi
